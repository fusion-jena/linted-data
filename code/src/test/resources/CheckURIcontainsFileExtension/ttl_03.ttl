@prefix : <http://www.city.ac.uk/ds/inm713/gr#> .
@prefix gr: <http://www.city.ac.uk/ds/inm713/gr#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.city.ac.uk/ds/inm713/gr#> .

###  http://www.city.ac.uk/ds/inm713/gr#StreetName
gr:StreetName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain gr:address ;
              rdfs:range xsd:string ;
              rdfs:comment "Name der Strasse des Restaurants."@de ;
              rdfs:label "Strassenname"@de ,
                         "streetname.ttl"@en .
						 
###  http://www.city.ac.uk/ds/inm713/gr.ttl#Country
gr:Country rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty gr:Name ;
                             owl:allValuesFrom xsd:string
                           ] ;
           rdfs:comment "Land in dem ein Restaurant stehen kann"@de ;
           rdfs:label "Country"@en ,
                      "Land"@de .


###  http://www.city.ac.uk/ds/inm713/gr#State
gr:State rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty gr:partOfCountry ;
                           owl:allValuesFrom gr:Country
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty gr:Name ;
                           owl:allValuesFrom xsd:string
                         ] ;
         rdfs:comment "Ein Teil eines Landes"@de ;
         rdfs:label "Staat"@de ,
                    "State"@en .

###  http://www.city.ac.uk/ds/inm713/gr#chilli
gr:chilli rdf:type owl:Class ;
          rdfs:subClassOf gr:vegetableTopping ;
          rdfs:comment "Chilli"@en ,
                       "Peperoni in Scheiben geschnitten"@de ;
          rdfs:label "Peperoni"@de .		

gr:Pizza_87  rdf:type  gr:Pizza ;
        <http://www.example/foo.ttl#Name> "chicken alfredo pizza" ;
        gr:hasPrice    gr:Price_87 ;
        gr:hasTopping  gr:chicken_87_0 .

gr:category_520_1  rdf:type  gr:category ;
        gr:Name   "cellular phone company" .

gr:Local_781  rdf:type  gr:Local ;
        gr:Name         "buca di beppo" ;
        gr:hasAddress   gr:address_781 ;
        gr:hasCategory  gr:category_33_6 , gr:category_47_0 , gr:italianRestaurant_57_4 , gr:category_781_1 , gr:restaurant_21_1 ;
        gr:serves       gr:Pizza_781 .

gr:Price_2455  rdf:type  gr:Price ;
        gr:hasCurrency  gr:Currency_USD ;
        gr:hasValue     "12.0"^^xsd:double .		  